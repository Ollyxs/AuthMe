{
  "datacenter": "dc",
  "domain": "consul",
  "node_name": "ds",
  "server": true,
  "bootstrap": true,
  "log_level": "DEBUG",
  "primary_datacenter": "dc",
  "ui_config": {
    "enabled": true
  },
  "acl":{
    "enabled": false
  },
  "connect": {"enabled": true},
  "disable_update_check": true,
  "client_addr": "0.0.0.0",
  "enable_script_checks": false,
  "disable_remote_exec": true,
  "tls": {
    "defaults": {
      "verify_incoming": false,
      "verify_outgoing": false
    },
    "internal_rpc": {
      "verify_server_hostname": false
    }
  },
  "auto_encrypt": {
    "allow_tls": false
  },
  "addresses": {
    "https": "0.0.0.0"
  },
  "ports": {
    "dns": 8600,
    "http": 8500,
    "https": -1,
    "server": 8300
  }
}